{
  "name": "posternode",
  "version": "2.2.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "coverage": "nyc --reporter=html -n src -e .ts bin/test",
    "preversion": "npm test -s",
    "format": "prettier --write '**/*.{js,ts,json,css,md}'",
    "lint": "tsc --noEmit",
    "start": "node -r dotenv/config dist",
    "test": "bin/test",
    "dev": "node --require ts-node/register --require dotenv/config src/cli.ts",
    "debug": "node  --inspect-brk --require ts-node/register --require dotenv/config src/cli.ts",
    "gen-toc": "npx md-toc -f '**/*.md' -i"
  },
  "dependencies": {
    "@robb_j/chowchow": "^0.5.4",
    "@robb_j/chowchow-auth": "^0.2.1",
    "@robb_j/chowchow-json-envelope": "^0.2.3",
    "@robb_j/chowchow-logger": "^0.3.1",
    "commander": "^9.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "escape-string-regexp": "^4.0.0",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.1.0",
    "mysql": "^2.18.1",
    "ndjson": "^2.0.0",
    "node-fetch": "^2.6.7",
    "pdfkit": "^0.13.0",
    "pug": "^3.0.2",
    "serve-favicon": "^2.5.0",
    "twilio": "^3.77.3",
    "valid-env": "^1.1.1"
  },
  "devDependencies": {
    "@robb_j/md-toc": "^0.3.3",
    "@types/chai": "^4.3.1",
    "@types/cors": "^2.8.12",
    "@types/download": "^8.0.1",
    "@types/escape-string-regexp": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.1",
    "@types/mysql": "^2.15.21",
    "@types/ndjson": "^2.0.1",
    "@types/node": "^18.0.0",
    "@types/node-fetch": "^2.6.2",
    "@types/pdfkit": "^0.12.6",
    "@types/pug": "^2.0.6",
    "@types/serve-favicon": "^2.5.3",
    "@types/supertest": "^2.0.12",
    "chai": "^4.3.6",
    "husky": "^5.2.0",
    "lint-staged": "^13.0.2",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "serialport": "^10.5.0",
    "sqlite3": "^5.0.8",
    "supertest": "^6.2.3",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,css,md}": [
      "prettier --write"
    ]
  }
}
