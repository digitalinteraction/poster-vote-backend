{
  "name": "posternode",
  "version": "2.0.0-beta.2",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "coverage": "nyc --reporter=html -n src -e .ts bin/test",
    "mysql": "docker run -it --rm --name dev_mysql -v mysql:/var/lib/mysql -e MYSQL_ROOT_PASSWORD=secret -p 3306:3306 mysql:5.7",
    "preversion": "npm test -s",
    "prettier": "prettier --write '**/*.{js,ts,json,css,md}'",
    "lint": "tsc --noEmit",
    "start": "node -r dotenv/config dist",
    "test": "bin/test",
    "dev": "nodemon -w src -e ts -x 'bin/tsnode' -q src/index.ts",
    "gen-toc": "npx md-toc -f '**/*.md' -i"
  },
  "author": "robb-j",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@robb_j/chowchow": "^0.5.4",
    "@robb_j/chowchow-auth": "^0.2.0",
    "@robb_j/chowchow-json-envelope": "^0.2.0",
    "@robb_j/chowchow-logger": "^0.3.0",
    "@sendgrid/mail": "^6.3.1",
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "download": "^7.1.0",
    "escape-string-regexp": "^1.0.5",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.16.3",
    "mysql": "^2.16.0",
    "pdfkit": "^0.9.0",
    "pug": "^2.0.3",
    "serve-favicon": "^2.5.0",
    "twilio": "^3.29.2",
    "valid-env": "^0.1.3"
  },
  "devDependencies": {
    "@robb_j/md-toc": "^0.3.0",
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.4",
    "@types/download": "^6.2.4",
    "@types/escape-string-regexp": "^1.0.0",
    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mocha": "^5.2.6",
    "@types/mysql": "^2.15.5",
    "@types/node": "^10.14.4",
    "@types/pdfkit": "^0.7.36",
    "@types/pug": "^2.0.4",
    "@types/serve-favicon": "^2.2.30",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "gen-md-toc": "^0.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "prettier": "1.16.4",
    "sqlite3": "^4.0.6",
    "supertest": "^4.0.2",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.4000"
  }
}
